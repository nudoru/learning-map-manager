{
  "name": "react-starter-2-app",
  "version": "0.1.0",
  "description": "Starting point for React 15, Babel 6 and Webpack beta 2 projects",
  "engines": {
    "node": ">=4"
  },
  "main": "./server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "dev:server": "nodemon server.js",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "clean-www": "rm -rf ./front/www/",
    "predev": "npm run clean-www",
    "dev": "webpack-dev-server --hot --env.dev --history-api-fallback --host=0.0.0.0 --port 3000 --content-base front/www/",
    "prebuild": "npm run clean-www",
    "prebuild:prod": "npm run clean-www",
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod -p",
    "lint": "eslint .",
    "setup": "npm install && npm run validate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nudoru/React-Starter-2.git"
  },
